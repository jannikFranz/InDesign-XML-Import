function addWorkIndex(index, workIds, type) {
    const subtopicTitles = {"works": "Musikalische Werke", "arrangements": "Bearbeitungen", "writings": "Schriften"}
    let subtopicTitle = subtopicTitles[type]
    
    for (let i = 0; i < workIds.length; i++) {
    //alert("Neues Werk: " + workIds[i] + ", Modus " + type)
    try {
        let id = workIds[i]
        let title, authorId, authorName, paramountWork, topic_work, topic_person, subordinateWork
        for (const [persId, persObject] of Object.entries(listPersons)) {
            // Ist die aktuelle Werk-ID als key unter dem entsprechenden type zu finden?
            if (id in persObject[type]) {
                title = (persObject[type][id] instanceof Object) ? persObject[type][id]["title"] : persObject[type][id]
                //alert("Werk mit ID " + id + " gefunden (" + title + ")")
                authorId = persId
                authorName = persObject["persName"]
                subordinateWork = false
            } else if (type == "works") {
                for (const [workId, workObject] of Object.entries(persObject[type])) {
                    if (workObject instanceof Object && id in workObject["related"]) {
                        title = workObject["related"][id]
                        authorId = persId
                        authorName = persObject["persName"]
                        subordinateWork = true
                        prarmountWorkId = workId
                        paramountWork = workObject["title"]
                        //alert(title + " gefunden als untergeordnetes Werk von " + paramountWork + "(" + authorName + ")")
                    }
                }
            } else {}
        }

        //alert("Gefundenes Werk: " + title + " von " + authorName + "(" + id + ")")
        // if no work is found (e. g. compositions in mode "arrangement") then continue with next work
        if (title === undefined) { continue }

        //alert("Titel: " + title)

        // create index entry for author if not already existing
        if (!index.topics.itemByName(authorName).isValid) {
            topic_person = index.topics.add(authorName)
            //alert("Eintrag für " + authorName + " angelegt")
        }

        if (!index.topics.itemByName(authorName).topics.itemByName(subtopicTitle).isValid) {
            // add subcategories "Musikalische Werke", "Bearbeitungen" and/or "Schriften" if needed
            let subtopicCounter = 0
            let checkForSubtopics = Object.keys(subtopicTitles).map(key => {
                if (Object.keys(listPersons[authorId][key]).length !== 0) {
                    subtopicCounter++
                }
            })
            if (subtopicCounter > 1) {
                for (const [type, title] of Object.entries(subtopicTitles)) {
                    if (Object.keys(listPersons[authorId][type]).length !== 0) {
                        topic_workCategory = index.topics.itemByName(authorName).topics.add(subtopicTitle)
                    }
                }
            }
        }

        // create index entry for category if not already existing for this author
        /*if (!index.topics.itemByName(authorName).topics.itemByName(subtopicTitle).isValid) {
            topic_workCategory = index.topics.itemByName(authorName).topics.add(subtopicTitle)
            //alert("Eintrag für Unterkategorie " + subtopicTitle + " angelegt")
        }*/
        // create index entry for work if current workId is a subodinate Work
        let indexRoot = index.topics.itemByName(authorName).topics.itemByName(subtopicTitle).isValid ? index.topics.itemByName(authorName).topics.itemByName(subtopicTitle) : index.topics.itemByName(authorName)

        if (subordinateWork === true) {
            //alert("untergeordnetes Werk anlegen")
            // if there is no index entry for the paramount work, create an entry
            if (!indexRoot.topics.itemByName(paramountWork).isValid) {
                topic_workCategory = indexRoot.topics.add(paramountWork)
            }
            // new entry for the subordinate work
            topic_work = indexRoot.topics.itemByName(paramountWork).topics.add(title)
        } else {
            if (indexRoot.topics.itemByName(title).isValid) {
                //alert("Werkeintrag existiert bereits im Index")
                topic_work = indexRoot.topics.itemByName(title)
            } else {
                //alert("normales Werk anlegen: " + title + " von " + authorName + " in Kategorie " + subtopicTitle)
                topic_work = indexRoot.topics.add(title)
            }
        }

        // get all occurrences where the work is tagged
        const allOccurrences = allWorkTags.filter(work => work.xmlAttributes.itemByName("key").value == id)
        
        // create a page reference for each occurrence
        for (let k = 0; k < allOccurrences.length; k++) {
            try {
                const text = allOccurrences[k].texts.everyItem()
                const pageReference = topic_work.pageReferences.add(text, PageReferenceType.CURRENT_PAGE);
                // ### NEU 23.11.2025 ###
                let parentElement = allOccurrences[k].parent
                while (parentElement.constructor.name === "XMLElement") {
                    if (parentElement.markupTag.name === "comment") {
                        pageReference.label = "note"
                        pageReference.pageNumberStyleOverride = insideNote
                        break
                    }
                    parentElement = parentElement.parent
                }
                // #######################
            } catch(err) {
                alert("Fehler beim Anlegen von Seitenreferenzen für Werk " + id + ": " + err)
                notFound.push(id)
            }
        }
        //alert("Prozess für dieses Werk abgeschlossen; Seitenreferenzen: " + topic_work.pageReferences.length)
        

    } catch (err) {
        alert("Fehler an dieser Stelle: " + err)
    }
}}

function addSecondLevelIndex(index, entityIds, type) {
    // *********************************************
    // add index topic for each work / writing in the document
    // *********************************************
    for (let i = 0; i < entityIds.length; i++) {

    try {

        // get PersObject that contains the work
        for (const [key, value] of Object.entries(listPersons)) {
            // sämtliche übergeordneten keys werden benötigt => Autor-ID und Name sowie ggf. übergeordnetes Werk ID und Name!
            let authorId = key
            let persObject = value
            if (entityIds[i] in persObject[type]) {
                let authorName = persObject.persName
                let workTitle = persObject[type][entityIds[i]]
                let topic_work

                // check if there is already a topic for the author
                if (index.topics.itemByName(authorName).isValid) {
                    if (showOnlyWorks.includes(authorId)) {
                        const subTopicType = subtopicTitles[type]
                        topic_work = index.topics.itemByName(authorName).topics.itemByName(subTopicType).topics.add(workTitle)
                    } else {
                        topic_work = index.topics.itemByName(authorName).topics.add(workTitle)
                    }
                } else {
                    topic_person = index.topics.add(authorName)
                    //alert("Eintrag für " + authorName + " angelegt")
                    if (showOnlyWorks.includes(authorId)) {
                        const subTopicType = subtopicTitles[type]
                        topic_person.topics.add(subTopicType)
                        topic_work = topic_person.topics.itemByName(subTopicType).topics.add(workTitle)
                    } else {
                        topic_work = topic_person.topics.add(workTitle)
                    }
                }

                // get all occurrences where the work is tagged
                const allOccurrences = (type === "writings") ? allWritingTags.filter(work => work.xmlAttributes.itemByName("key").value == entityIds[i]) : allWorkTags.filter(work => work.xmlAttributes.itemByName("key").value == entityIds[i])
                            
                // create a page reference for each occurrence
                for (let k = 0; k < allOccurrences.length; k++) {
                    try {
                        const text = allOccurrences[k].texts.everyItem()
                        topic_work.pageReferences.add(text, PageReferenceType.CURRENT_PAGE);
                    } catch(err) {
                        alert("Fehler beim Anlegen von Seitenreferenzen (Werke): " + err)
                        notFound.push(entityIds[i])
                    }
                }
            }
        }
    } catch (err) {
        alert(err)
        notFound.push(entityIds[i])
        }
    }
}

